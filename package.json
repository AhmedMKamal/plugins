{
  "name": "lipsurf-plugins",
  "version": "2.0.0",
  "description": "Plugins for the LipSurf Chrome extension. Each plugin adds a set of commands to LipSurf.",
  "scripts": {
    "docs:watch": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "lipsurf-cli build && yarn clean",
    "watch": "lipsurf-cli build --watch",
    "build:prod": "NODE_ENV=production lipsurf-cli build && yarn clean",
    "watch:prod": "NODE_ENV=production lipsurf-cli build --watch",
    "clean": "rm dist/*.js dist/*.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lipsurf/plugins"
  },
  "keywords": [
    "voice",
    "recognition",
    "hands-free",
    "abilities",
    "accessibility"
  ],
  "author": "Miko",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lipsurf/plugins/issues"
  },
  "homepage": "https://www.lipsurf.com",
  "devDependencies": {
    "@types/chrome": "0.0.66",
    "@types/jquery": "^3.3.2",
    "@types/selenium-webdriver": "^3.0.8",
    "lipsurf-cli": "https://github.com/lipsurf/lipsurf-cli",
    "lipsurf-plugin-types": "https://github.com/lipsurf/plugin-types",
    "vuepress": "^0.14.2"
  }
}
