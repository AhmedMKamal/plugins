{
  "compilerOptions": {
    "declaration": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": false,
    "target": "ES2017"
  }
}
